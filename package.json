{
  "name": "sf-fetch",
  "author": "Samuel Formigheri",
  "version": "1.1.0",
  "description": "Simple lib to implement cache on fetch requests",
  "keywords": [
    "fetch",
    "cache"
  ],
  "repository": "SamuelFormigheri/sf-fetch",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsup src/lib/sf-fetch.ts --format esm,cjs --dts",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@types/node": "^18.13.0",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^6.6.3",
    "typescript": "^4.9.3",
    "vite": "^4.1.0"
  }
}